#include <iostream>
using namespace std;
// tugas praktikum bahasa pemograman lilis muliani
int main (){
	//menampilkan angka bilangan ganjil dan genap 100-1000
	int angka_awal,angka_akhir;
	cout << "menampilkan bilangan ganjil dan genap 100-1000 : " << endl;
	cin >> angka_awal;
	
	for ( int a = 100; a <= 1000; a++){
		if ( a % 2 == 1 ){
			cout << a << " : adalah bilangan ganjil " << endl;
		} else {
			cout << a << " : adalah bilangan genap " << endl;
		}
	}
}
